    },
    "Properties": {
      "Application": "MHS.P4.OnlineReferrals"
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "E:\\PcsTfs\\MHS.P4\\MHS.P4.CIT\\MHS.CIT.Web\\Logs\\log-{Date}.txt",
          "retainedFileCountLimit": 14,
          "buffered": false,
          "outputTemplate": "[[{Timestamp:HH:mm:ss} {Level} {MachineName} {EnvironmentUserName}] {Message}{NewLine}{Exception}]"
        }
      },
      {
